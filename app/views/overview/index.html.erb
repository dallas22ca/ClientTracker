<% if @user.contacts.count == 0 %>

	<%= render "quickstart" %>

<% else %>

	<%= render "time" %>

	<div style="background: #eee; width: 64.5%; margin-top: 1%; margin-right: 0.5%; float: left; padding: 20px; ">
		- Event totals by event type
		<%= @user.events.group_by(&:description) %>
	</div>

	<div style="background: #eee; width: 34.5%; margin-top: 1%; margin-left: 0.5%; float: left; padding: 20px; ">
		RPM: <%= @user.events.count %>
	</div>

	<div class="clear"></div>

<% end %>